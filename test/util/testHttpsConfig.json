{
  "port": 1234,
  "makeBuildArtifacts": false,
  "logging": {
    "methods": {
      "http": false,
      "info": false,
      "warn": false
    }
  },
  "secretsFolder": "./test/app/certs",
  "https": {
    "enable": true,
    "force": true,
    "autoCert": true,
    "port": 2468,
    "authInfoPath": {
      "p12": {
        "p12Path": "test/app/certs/cert.p12"
      },
      "authCertAndKey": {
        "cert": "test/app/certs/cert.pem",
        "key": "test/app/certs/key.pem"
      }
    },
    "passphrase": "testpass",
    "caCert": "test/app/certs/cert.pem",
    "requestCert": false,
    "rejectUnauthorized": false
  }
}
